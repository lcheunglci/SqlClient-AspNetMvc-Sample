@model IEnumerable<WebAppFxData.Models.Employee>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>   There are @Model.Count() number of employees in the db</div>

    @DateTime.Now.Year
</div>

<div>
    We have the following employees: 
</div>

<ul>
    @foreach (var employee in Model)
    {
        <li>@employee.FirstName @employee.LastName</li>
    }
</ul>
